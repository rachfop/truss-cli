{
  "json": {
  },
  "markdown": {
    "lineWidth": 80,
    "newLineKind": "lf",
    "textWrap": "maintain",
    "emphasisKind": "underscores",
    "strongKind": "asterisks",
    "ignoreDirective": "dprint-ignore",
    "ignoreFileDirective": "dprint-ignore-file",
    "ignoreStartDirective": "dprint-ignore-start",
    "ignoreEndDirective": "dprint-ignore-end"
  },
  "markup": {
    "lineWidth": 80,
    "newLineKind": "lf"
  },
  "ruff": {
  },
  "yaml": {
  },
  "includes": [
    "**/*.md",
    "**/*.mdx"
  ],
  "excludes": [
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/json-0.21.0.wasm",
    "https://plugins.dprint.dev/markdown-0.20.0.wasm",
    "https://plugins.dprint.dev/ruff-0.6.1.wasm",
    "https://plugins.dprint.dev/g-plane/markup_fmt-v0.24.0.wasm",
    "https://plugins.dprint.dev/g-plane/pretty_yaml-v0.5.1.wasm"
  ]
}
